[package]
name = "yachtsql-sqlparser-derive"
description = "Procedural (proc) macros for yachtsql-sqlparser"
version = "0.3.0"
authors = ["sqlparser-rs authors", "Alex Choi <alexchoi@hey.com>"]
homepage = "https://github.com/alexchoi0/yachtsql-sqlparser"
documentation = "https://docs.rs/yachtsql-sqlparser-derive/"
keywords = ["ansi", "sql", "lexer", "parser", "bigquery"]
repository = "https://github.com/alexchoi0/yachtsql-sqlparser"
license = "Apache-2.0"
include = [
    "src/**/*.rs",
    "Cargo.toml",
    "LICENSE.TXT",
]
edition = "2021"

[lib]
proc-macro = true

[dependencies]
syn = { version = "2.0", default-features = false, features = ["printing", "parsing", "derive", "proc-macro"] }
proc-macro2 = "1.0"
quote = "1.0"
